# API Configuration
# Replace with your actual API domain
NEXT_PUBLIC_API_DOMAIN_V1=https://your-api-domain.com/v1

# VDP URL
NEXT_PUBLIC_VDP_URL=https://your-vdp-url.com

# App URLs
NEXT_PUBLIC_APP_URL=https://your-app-url.com
NEXT_PUBLIC_VIOME_APP_URL=https://your-viome-app-url.com

# Environment
NEXT_PUBLIC_VERCEL_APP_ENV=production
NODE_ENV=production

# Optional: Statsig (Feature Flags)
# NEXT_PUBLIC_STATSIG_KEY=your_statsig_key
# NEXT_PUBLIC_STATSIG_ENV=production
